    "version": "2.0.0",
    "tasks": [
        {
            "label": "RunApp",
            "type": "shell",
            "command": ".venv/bin/python -m entrypoint",
            "problemMatcher": [],
        },
        {
            "label": "Build",
            "type": "shell",
            "command": "poetry",
            "args": [
                "run",
                "pyinstaller",
                "entrypoint.py",
                "-y"
            ],
        },
    ]
}